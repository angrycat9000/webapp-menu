{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": [
          "jsdoc",
          "closure"
      ]
  },
  "source": {
      "include": [
          "./src"
      ],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts":{
    "template": "node_modules/braintree-jsdoc-template"
  },
  "templates": {
      "referenceTitle": "Webapp Menu",
      "cleverLinks": true,
      "monospaceLinks": false
  },
  "metadata": {
      "title": "Webapp Menu"
  }
}