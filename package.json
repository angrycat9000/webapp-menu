{
  "name": "webapp-menu",
  "version": "3.0.0",
  "description": "Create accessible menus for web apps (eg. file, edit, context menus)",
  "main": "dist/webapp-menu.js",
  "scripts": {
    "test": "npm run build:code && karma start --coverage",
    "test:quick": "karma start",
    "test:debug": "karma start --no-single-run --browsers=Chrome",
    "start": "rollup -w -c rollup.config.js & start-storybook -p 6006",
    "start:site": "es-dev-server --root-dir site --open",
    "build":"npm run build:dist && npm run build:site",
    "build:site": "rimraf site && npm run build:storybook && npm run build:jsdoc && npm run build:site-homepage",
    "build:dist": "rimraf dist && npm run build:code && npm run build:custom-elements",
    "build:site-homepage": "cp dist/* site/  && cp src/index.html site/index.html && cp src/index.js site/index.js && cp -r screenshots site/docs/screenshots",
    "build:custom-elements": "wca analyze src --format json --outFile dist/custom-elements.json",
    "build:jsdoc": "jsdoc -c ./jsdoc.json -d ./site/docs/ -R ./README.md -a public",
    "build:code": " rollup -c rollup.config.js --configProduction",
    "build:code-debug": "rollup -c rollup.config.js",
    "build:storybook": "build-storybook -o site/storybook",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angrycat9000/webapp-menu.git"
  },
  "author": "Mark Dane",
  "contributors": [
    "Alex Umstead"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angrycat9000/webapp-menu/issues"
  },
  "homepage": "https://github.com/angrycat9000/webapp-menu#readme",
  "devDependencies": {
    "@open-wc/semantic-dom-diff": "^0.14.3",
    "@open-wc/testing": "^2.5.32",
    "@open-wc/testing-karma": "^4.0.9",
    "@storybook/addon-actions": "^6.0.19",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-storysource": "^6.0.19",
    "@storybook/web-components": "^6.0.19",
    "braintree-jsdoc-template": "^3.3.0",
    "cssnano": "^5.1.13",
    "deepmerge": "^3.2.0",
    "es-dev-server": "^1.60.2",
    "eslint": "^8.23.0",
    "jsdoc": "^4.0.0",
    "karma": "^6.3.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.4.16",
    "rimraf": "^3.0.0",
    "rollup": "2.78.0",
    "rollup-plugin-license": "^2.8.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sass": "^1.2.13",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "taffydb": "^2.7.3",
    "web-component-analyzer": "^1.1.6"
  },
  "keywords": [
    "menu",
    "context menu",
    "menubar",
    "toolbar",
    "ui",
    "accessible",
    "accessibility",
    "a11y",
    "webapp",
    "popup",
    "context toolbar"
  ]
}
