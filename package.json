{
  "name": "webapp-menu",
  "version": "3.0.0",
  "description": "Create accessible menus for web apps (eg. file, edit, context menus)",
  "main": "dist/webapp-menu.js",
  "scripts": {
    "test": "npm run build:code && karma start --coverage",
    "test:quick": "karma start",
    "test:debug": "karma start --no-single-run --browsers=Chrome",
    "start": "rollup -w -c rollup.config.js & es-dev-server --app-index dist/index.html  --watch --open",
    "develop": "rimraf dist && rollup -c rollup.config.js",
    "build:site": "npm run build && npm run build:storybook --",
    "build:docs": "wca analyze src --format json --outFile dist/custom-elements.json",
    "build:code": " rollup -c rollup.config.js --configProduction",
    "build": "rimraf dist && npm run build:code && npm run build:docs",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o dist/storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angrycat9000/webapp-menu.git"
  },
  "author": "Mark Dane",
  "contributors": [
    "Alex Umstead"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angrycat9000/webapp-menu/issues"
  },
  "homepage": "https://github.com/angrycat9000/webapp-menu#readme",
  "dependencies": {
    "tiny-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@open-wc/semantic-dom-diff": "^0.14.3",
    "@open-wc/testing": "^2.5.32",
    "@open-wc/testing-karma": "^4.0.9",
    "@storybook/addon-actions": "^6.0.19",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-storysource": "^6.0.19",
    "@storybook/web-components": "^6.0.19",
    "babel-loader": "^8.2.2",
    "cssnano": "^4.1.10",
    "deepmerge": "^3.2.0",
    "es-dev-server": "^1.60.2",
    "karma": "^6.3.1",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.35",
    "rimraf": "^3.0.0",
    "rollup": "^1.25.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^5.3.1",
    "web-component-analyzer": "^1.1.6"
  },
  "keywords": [
    "menu",
    "toolbar",
    "ui",
    "accessible",
    "webapp",
    "popup",
    "context menu",
    "context toolbar"
  ]
}
