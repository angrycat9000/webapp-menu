{
  "name": "webapp-menu",
  "version": "2.0.3",
  "description": "Create accessible menus for web apps (eg. file, edit, context menus)",
  "main": "dist/webapp-menu.js",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "start": "rollup -w -c rollup.config.js & es-dev-server --app-index dist/index.html  --watch --open",
    "develop": "rimraf dist && rollup -c rollup.config.js",
    "build:library": "rimraf dist && rollup -c rollup.config.js --configProduction",
    "build": "npm run build:library && npm run build:storybook",
    "prepare": "npm run build:library",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o dist/storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angrycat9000/webapp-menu.git"
  },
  "author": "Mark Dane",
  "contributors": [
    "Alex Umstead"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angrycat9000/webapp-menu/issues"
  },
  "homepage": "https://github.com/angrycat9000/webapp-menu#readme",
  "dependencies": {
    "tiny-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@open-wc/semantic-dom-diff": "^0.14.1",
    "@open-wc/testing": "^2.3.3",
    "@open-wc/testing-karma": "^3.1.33",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-storysource": "^5.1.11",
    "@storybook/html": "^5.1.11",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "deepmerge": "^4.0.0",
    "es-dev-server": "^1.17.2",
    "karma": "^4.3.0",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.18",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-index-html": "^1.5.3",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "keywords": [
    "menu",
    "toolbar",
    "ui",
    "accessible",
    "webapp",
    "popup",
    "context menu",
    "context toolbar"
  ]
}
